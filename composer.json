{
  "name": "joepdooper/ivy-roots",
  "description": "A set of classes designed to build and extend functionality in ivy",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
        "Ivy\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Joep Dooper"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.3.0",
    "ext-pdo": "*",
    "delight-im/auth": "^8.3",
    "delight-im/db": "^1.3",
    "phpmailer/phpmailer": "^6.4",
    "verot/class.upload.php": "^2.1",
    "bramus/router": "^1.6",
    "vlucas/phpdotenv": "^5.5",
    "wixel/gump": "^2.2",
    "latte/latte": "^3.0",
    "symfony/http-foundation": "^7.3",
    "matthiasmullie/minify": "^1.3",
    "php-curl-class/php-curl-class": "^9.6",
    "bainternet/php-hooks": "^1.0",
    "samdark/sitemap": "^2.4",
    "predis/predis": "^3.0@alpha",
    "ezyang/htmlpurifier": "dev-master"
  },
  "require-dev": {
    "pestphp/pest": "3.x-dev",
    "phpstan/phpstan": "2.0.x-dev",
    "kint-php/kint": "dev-master",
    "joepdooper/ivy-cultivate": "dev-main"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}