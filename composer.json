{
    "name": "joepdooper/ivy-roots",
  "description": "a collection of core classes for building and extending functionality with Ivy",
    "type": "library",
    "license": "MIT",
    "autoload": {
    "psr-4": {
        "Ivy\\": "src/"
    }
},
    "authors": [
        {
            "name": "Joep Dooper"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.2.0",
        "ext-pdo": "*",
        "ezyang/htmlpurifier": "^4.13",
        "delight-im/auth": "^8.3",
        "delight-im/db": "^1.3",
        "phpmailer/phpmailer": "^6.4",
        "verot/class.upload.php": "^2.1",
        "bramus/router": "^1.6",
        "vlucas/phpdotenv": "^5.5",
        "wixel/gump": "dev-master",
        "latte/latte": "^3.0@dev"
    },
    "require-dev": {
        "pestphp/pest": "3.x-dev",
        "phpstan/phpstan": "2.0.x-dev"
    }
}
